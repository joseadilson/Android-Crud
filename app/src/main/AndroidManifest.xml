<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jose.crud_cadastro">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Tarefas"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="Tarefas" />
        <activity
            android:name=".CadUsuarioActivity"
            android:label="Cadastro de usuarios">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".ListaUsuariosActivity" />
        </activity>
        <activity
            android:name=".ListaUsuariosActivity"
            android:label="Usuarios">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity android:name=".CadTarefasActivity"
            android:label="Cadastro de tarefas">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
    </application>

</manifest>